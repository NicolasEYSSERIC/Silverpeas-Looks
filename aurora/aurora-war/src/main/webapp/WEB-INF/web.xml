<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app  xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
version="2.4" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>Look Aurora</display-name>
  <servlet>
    <servlet-name>WeatherService</servlet-name>
    <servlet-class>org.silverpeas.looks.aurora.servlets.WeatherServiceAdapter</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GoToSpaceHomepageBackoffice</servlet-name>
    <servlet-class>org.silverpeas.looks.aurora.servlets.GoToSpaceHomepageBackOffice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WeatherService</servlet-name>
    <url-pattern>/RWeatherService/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoToSpaceHomepageBackoffice</servlet-name>
    <url-pattern>/AuroraSpaceHomepageBackoffice</url-pattern>
  </servlet-mapping>
</web-app>